{
  "trailingComma": "es5",
  "tabWidth": 2,
  "bracketSameLine": true,
  "bracketSpacing": true,
  "useTabs": true,
  "semi": true,
  "singleQuote": true,
  "printWidth": 120,
  "embeddedLanguageFormatting": "auto",
  "jsxSingleQuote": false,
  "arrowParens": "always",
  "endOfLine": "lf",
  "plugins": [
    "prettier-plugin-tailwindcss"
  ]
}