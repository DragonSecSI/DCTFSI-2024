    "autoprefixer": "^10.4.19",
    "concurrently": "^8.2.2",
    "nodemon": "^3.1.0",
    "postcss": "^8.4.38",
    "prettier-plugin-tailwindcss": "^0.5.13",
    "tailwindcss": "^3.4.3"
  }
